#include <string>
#include <stdio.h>
#include <string.h>
#include <iostream>
#include <algorithm>
#define MAXN 1000010
#define INF 0x3f3f3f3f
using namespace std;
int m;
char t[MAXN];
int nex[MAXN];
int sav[MAXN];
void get_next();
void save(int end);
int main() {
	int runs;
	while (~scanf("%d", &runs)) {
		for (int run = 1; run <= runs; run++) {
			int cnt;
			scanf("%s", t);
			m = strlen(t);
			get_next();
			cnt = 0;
			for (int j = m; j > 0; j = nex[j], cnt++) {
				sav[cnt] = m - nex[j];
			}
			printf("Case #%d: %d\n", run, cnt);
			for (int i = 0; i < cnt - 1; i++) {
				printf("%d ", sav[i]);
			}
			printf("%d\n", sav[cnt - 1]);
		}
	}
	return 0;
}

void get_next() {
	nex[0] = -1;
	int j = 0, k = -1;
	while (j < m) {
		if (k == -1 || t[j] == t[k]) {
			nex[++j] = ++k;
		}
		else {
			k = nex[k];
		}
	}
}
