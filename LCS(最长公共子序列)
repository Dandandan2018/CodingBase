#include <queue>
#include <vector>
#include <string>
#include <stdio.h>
#include <string.h>
#include <iostream>
#include <algorithm>
#define MAXN 1010
using namespace std;
int n, m;
string s1, s2;
int dp[MAXN][MAXN];
void Solve(int cnt, int v0);
int main() {
	while (scanf("%d%d", &n, &m) != EOF) {
		cin >> s1 >> s2;
		Solve(0, m);
	}
	return 0;
}

void Solve(int i, int j) {
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < m; j++) {
			if (s1[i] == s2[j]) {
				dp[i + 1][j + 1] = dp[i][j] + 1;
			}
			else {
				dp[i + 1][j + 1] = max(dp[i][j + 1], dp[i + 1][j]);
			}
		}
	}
	printf("%d\n", dp[n][m]);
}
