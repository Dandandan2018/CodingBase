#include <queue>
#include <vector>
#include <string>
#include <stdio.h>
#include <string.h>
#include <iostream>
#include <algorithm>
#define MAXN 10010
using namespace std;
int n, m;
int v[MAXN], w[MAXN];
int dp[2][MAXN];
void Solve();
int main() {
	while (scanf("%d%d", &n, &m) != EOF) {
		memset(dp, 0, sizeof(dp));
		for (int i = 0; i < n; i++) {
			scanf("%d%d", &w[i], &v[i]);
		}
		Solve();
	}
	return 0;
}

void Solve() {
	for (int i = 0; i < n; i++) {
		for (int j = m; j >= w[i]; j--) {
			if (j < w[i]) {
				dp[(i + 1) & 1][j] = dp[i & 1][j];
			}
			else {
				dp[(i + 1) & 1][j] = max(dp[i & 1][j], dp[(i + 1) & 1][j - w[i]] + v[i]);
			}
		}
	}
	printf("%d", dp[n & 1][m]);
}
